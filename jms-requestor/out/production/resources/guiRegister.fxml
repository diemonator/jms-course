<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" />
        <ColumnConstraints hgrow="SOMETIMES" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints vgrow="SOMETIMES" />
        <RowConstraints vgrow="SOMETIMES" />
        <RowConstraints vgrow="SOMETIMES" />
        <RowConstraints vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Label fx:id="lb" text="Create Student Account" GridPane.columnSpan="2" GridPane.halignment="CENTER">
            <GridPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </GridPane.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding></Label>
        <Button fx:id="btnCreate" mnemonicParsing="false" text="Create Account" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="3">
         <GridPane.margin>
            <Insets bottom="15.0" top="8.0" />
         </GridPane.margin></Button>
        <TextField fx:id="tfName" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets bottom="8.0" right="15.0" top="8.0" />
            </GridPane.margin></TextField>
      <Label text="Username" GridPane.halignment="CENTER" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets bottom="8.0" left="15.0" right="15.0" top="8.0" />
         </GridPane.margin></Label>
      <Label text="Password" GridPane.halignment="CENTER" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets bottom="8.0" left="15.0" right="15.0" top="8.0" />
         </GridPane.margin></Label>
      <TextField fx:id="tfPass" GridPane.columnIndex="1" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets bottom="8.0" right="15.0" top="8.0" />
         </GridPane.margin>
      </TextField>
    </children>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
</GridPane>
